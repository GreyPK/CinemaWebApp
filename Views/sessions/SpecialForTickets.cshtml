@model IEnumerable<session>


<table class="table">
    <tr>
        <th>
            ID сеанса
        </th>
        <th>
            Дата
        </th>
        <th>
            Время
        </th>
        <th>
            Базовая цена
        </th>
        <th>
            Название фильма
        </th>
        <th>
            Номер кинозала
        </th>
        <th>
            Тип кинозала
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.id_session)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.date_session.Day).@Html.DisplayFor(modelItem => item.date_session.Month).@Html.DisplayFor(modelItem => item.date_session.Year)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.time_session)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.base_price)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.film.film_name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.room.number_room)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.room.room_type.name_type_room)
            </td>
        </tr>
    }

</table>